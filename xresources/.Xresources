Xcursor.theme: yaru
Xcursor.size: 16
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
!Xft.dpi:96

XTerm*faceName: Ubuntu Mono
XTerm*faceSize: 12
URxvt.font: xft:Ubuntu Mono:pixelsize=16,xft:FontAwesome:pixelsize=14,xft:Joypixels:pixelsize:14

!URxvt.letterSpace: -1
URxvt.linespace: 2

!URxvt*colorBD:            #ffffff
!URxvt*cursorColor:        #ff8939
!URxvt*cursorColor2:       #151515
!URxvt*highlightColor:     #ffffff
URxvt*borderless: True

URxvt.scrollBar: false
!URxvt.internalBorder: 10

! Dracula color scheme
URxvt.foreground: #F8F8F2
