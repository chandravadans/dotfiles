!! Extensions
!URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,tabbed,fullscreen,resize-font,selection-to-clipboard
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,fullscreen,resize-font,selection-to-clipboard
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 1

URxvt.keysym.F11: perl:fullscreen:switch
